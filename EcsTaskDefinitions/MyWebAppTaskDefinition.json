{
    "family": "MY_DEMO_TASK_FAMILY",
    "taskRoleArn": "THIS_TASK_ROLE_ARN",
    "containerDefinitions": [
        {
            "name": "MyWebContainer",
            "image": "CONTAINER_IMAGE",
            "cpu": 128,
            "memory": 128,
            "memoryReservation": 100,
            "portMappings": [
                {
                    "containerPort": CONTAINER_PORT,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "COLOUR",
                    "value": "PAGE_COLOUR"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "128",
    "memory": "128"
}
